#pragma once 
#define BUFFER_SIZE 6 

#define NORMAL_PREFIX    "N1"
#define AGED_PREFIX      "I2"
#define DEFICIENT_PREFIX "D3"
#define PREGNANNT_PREFIX "G4"

#include <cstring>
#include <stdio.h>

static enum _level {
    DEFICIENT,
    PREGNANT,
    AGED,
    NORMAL,
} level;

typedef struct _client{
    char pass[BUFFER_SIZE];
    enum _level priorityLevel;
} client;

const char* getDeficientPass(const int number);
const char* getPregnantPass(const int number);
const char* getAgedPass(const int number);
const char* getNormalPass(const int number);
const char* prepareNumber(const int number);
